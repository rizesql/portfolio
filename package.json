{
  "name": "portfolio",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && pnpm build:icons && astro build",
    "build:icons": "tsx ./src/lib/icons/generate-types.ts && tsx ./src/lib/icons/generate-sprite.ts",
    "preview": "astro preview",
    "preview:cloudflare": "wrangler pages dev ./dist",
    "astro": "astro",
    "format": "pnpm run /^format:/",
    "format:prettier": "prettier -w --plugin-search-dir=. .",
    "format:eslint": "eslint --fix \"**/*.{js,ts,jsx,tsx,mjs,cjs,astro}\" --cache --cache-location ./node_modules/.cache/eslint .",
    "lint": "pnpm run /^lint:/",
    "lint:prettier": "prettier --check --plugin-search-dir=. .",
    "lint:eslint": "eslint \"**/*.{js,ts,jsx,tsx,mjs,cjs,astro}\" --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:typescript": "tsc -b",
    "lint:astro": "astro check"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/cloudflare": "^10.4.2",
    "@astrojs/mdx": "^3.1.1",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/solid-js": "^4.4.0",
    "@astrojs/tailwind": "^5.1.0",
    "@kobalte/core": "0.13.3",
    "@kobalte/tailwindcss": "0.9.0",
    "@playform/compress": "^0.0.13",
    "@playform/inline": "^0.0.7",
    "@t3-oss/env-core": "0.10.1",
    "astro": "^4.11.0",
    "astro-compressor": "^0.4.1",
    "astro-meta-tags": "^0.3.0",
    "astro-svg-sprite": "^1.1.0",
    "cva": "1.0.0-beta.1",
    "date-fns": "3.6.0",
    "satori": "0.10.13",
    "sharp": "0.33.4",
    "solid-js": "^1.8.17",
    "solid-satori": "0.0.2",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@sly-cli/sly": "1.10.0",
    "@tailwindcss/typography": "0.5.13",
    "@types/fs-extra": "11.0.4",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "astro-eslint-parser": "^1.0.2",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-solid": "^0.14.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "fs-extra": "11.2.0",
    "glob": "10.4.2",
    "node-html-parser": "6.1.13",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tsx": "4.15.7",
    "wrangler": "^3.61.0"
  },
  "packageManager": "pnpm@9.4.0"
}
