{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  },
  "exclude": ["node_modules", "dist", ".wrangler"],
  "include": ["**/*", ".eslintrc"]
}
